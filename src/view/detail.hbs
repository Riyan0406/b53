<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
      integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/detail.css" />

  </head>
  <body>
    <nav class="navbar navbar-expand-lg" style="background-color: #004643">
      <div class="container">
        <a class="navbar-brand" href="#">
          <img
            class=""
            src="/image/3bf826d15ceb64dfc9d3712f5a451838-removebg-preview.png"
            alt=""
            style="width: 35px"
          />
        </a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a
                class="nav-link active text-light fw-bold"
                style="color: #eaa81b"
                aria-current="page"
                href="/"
              >Home</a>
            </li>
            <li class="nav-item">
              <a
                style="color: #eaa81b"
                class="nav-link text-light fw-bold"
                href="/add-project"
              >Add Project</a>
            </li>

            <li class="nav-item">
              <a
                style="color: #eaa81b"
                class="nav-link fw-bold"
                href="/testimonial"
              >My Testimonial</a>
            </li>
          </ul>
        </div>
        <a href="/contact" style="color: #004643"><button
            type="button"
            class="btn"
            ;
            style="background-color: #eaa81b"
          >
            Contact Me
          </button></a>
      </div>
    </nav>

    <!-- detail -->
    <div class="card-detail">
      <div class="judul-detail">
        <h2>Detail Project</h2>
      </div>
      <div class="detail">
        <div class="detail-left">
          <img src="/image/AI.avif" alt="" srcset="" />
        </div>
        <div class="detail-right">
          <h4>Duration</h4>
          <div class="duration">
            <i class="fa-solid fa-calendar-days"><p
              >{{dataDami.durasiProject}}</p></i>
            <i class="fa-solid fa-clock"><p>{{dataDami.postAt}}</p></i>
          </div>
          <h4>Technologies</h4>
          <div class="tecno">
            {{#each dataDami.Technologies}}
              <i
                style="font-size: 15px; display: flex; gap:1rem;"
                class="fa-brands fa-{{this}}"
              ><p>{{this}}</p></i>
            {{/each}}
          </div>
        </div>
      </div>
      <div class="detail-p">
        <p class="text-light fw-bold">
          {{dataDami.title}}
        </p>
        <p class="text-light fw-normal">
          {{dataDami.Description}}
        </p>
      </div>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"
  ></script>
</html>